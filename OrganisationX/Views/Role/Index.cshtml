@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>List of roles</h1>
<a asp-action="Create">Create</a>
<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var role in Model)
            {
            <td>@role.Id</td>
            <td>@role.Name</td>
            }
        </tr>
    </tbody>
    
</table>
